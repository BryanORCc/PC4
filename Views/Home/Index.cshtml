@model List<Publicacion>

@foreach (var item in Model)
{
    <div style="width: 100%; border-bottom: black 2px solid; margin-bottom: 5%;">
        <p><strong>Titulo: </strong> @item.Titulo</p>
        <p><strong>Subido por: </strong> @item.Nick</p>
        <div style="width: 40%;">
            <img width="70%" src="@item.Foto">
        </div>
    </div>
}
@if (Model.Count == 0) {
    <tr>
        <td colspan="12" class="text-center">
                <h3 style="color: blue;" class="m-5">No hay publicaciones</h3>
        </td>
    </tr>
}



