@model Login

<h1>LOGIN</h1>

<form action="" method="post">
    <div class="form-group">
        <label>ID:</label>
        <input asp-for="id" class="form-control" placeholder="Ingresar ID...">
    </div>

    <div class="form-group">
        <label>Contraseña:</label>
        <input asp-for="contraseña" type="password" class="form-control" placeholder="******">
    </div>
    <button class="btn btn-info">Ingresar</button>
    
    <p class="text-danger text-center mt-5">@ViewData["m"]</p>
</form>