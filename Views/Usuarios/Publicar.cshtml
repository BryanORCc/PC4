@model Publicacion

<h1>Publicar Fail</h1>

<form action="" method="post">

    <div class="form-group">
        <label asp-for="Titulo">Titulo de la publicacion</label>
        <input asp-for="Titulo" class="form-control" placeholder="ingresar titulo...">
    </div>

    <div class="form-group">
        <label asp-for="Foto">Url de la foto</label>
        <input asp-for="Foto" class="form-control" placeholder="ingresar url...">
    </div>

    <div class="form-group">
        <label asp-for="Nick">Nick</label>
        <input asp-for="Nick" class="form-control" placeholder="ingresar nick...">
    </div>
    
    <button class="btn btn-info">Publicar</button>
</form>

